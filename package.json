{
  "name": "news",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://BardAlexandrovskiy.github.io/news",
  "dependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "styled-components": "^4.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "react-app",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "jsx-quotes": [
        1,
        "prefer-double"
      ],
      "react/jsx-filename-extension": 0,
      "react/prop-types": 2
    },
    "plugins": [
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "gh-pages": "^2.1.1",
    "prettier": "^1.18.2"
  }
}